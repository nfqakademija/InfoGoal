{% extends "::base.html.twig" %}

{% block title %}Stalas:rezervuotas{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets filter='compass' output='bundles/infogoalkicker/css/*.css'
    '@InfoGoalKickerBundle/Resources/public/scss/Table/table.scss'

    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {% javascripts '@InfoGoalKickerBundle/Resources/public/js/reload.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block body %}

    <main class="container">
        <div id="table" class=" col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
            <img id="field-free" src="{{ asset('/bundles/infogoalkicker/assets/soccer-field.png') }}" alt="Soccer field" class="img-responsive">
            <div class="table-free">
                <p>Stalas rezervuotas!</p>
                <p>{{ mins }} : {{ secs }}</p>
            </div>
        </div><!-- end table -->
    </main><!-- end container -->
{% endblock %}